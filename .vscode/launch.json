{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "docs",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "http://localhost:3000/hpcc-js-wc/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "examples/iife",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "file://${workspaceFolder}/examples/iife/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "index.html",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "http://localhost:3000/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "file://index.html",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "file://${workspaceFolder}/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "utils/test",
            "program": "${workspaceFolder}/utils/test.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}