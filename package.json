{
  "name": "@hpcc-js/web-components",
  "version": "0.0.0",
  "description": "hpcc-js - Web Components Library",
  "private": true,
  "keywords": [],
  "sideEffects": false,
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "typeings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "vitepress-build": "vitepress build docs",
    "docs:serve": "vitepress serve --port 3000 docs",
    "docs:patch": "node utils/patch-docs.mjs",
    "clean": "rimraf build dist docs/.vitepress/dist docs/.vitepress/cache docs/api lib* types temp tmp *.tsbuildinfo ",
    "compile": "tsc -p ./tsconfig.json",
    "compile-watch": "npm run compile -- -w",
    "syntax-watch": "npm run compile -- -w --noEmit",
    "api-extractor": "api-extractor run --local --verbose",
    "api-extractor-watch": "nodemon --watch types \"run-s api-extractor\"",
    "api-documenter": "api-documenter markdown --input-folder ./temp --output-folder ./docs/api",
    "gen-api": "run-s compile api-extractor api-documenter",
    "copy-build": "copyfiles -u 1 \"./dist/index.min.js\" ./docs/.vitepress/dist/assets/",
    "copy-images": "copyfiles -u 2 \"./docs/images/**/*.{png,jpg,gif}\" ./docs/.vitepress/dist/images/",
    "copy-res": "run-p copy-build copy-images",
    "bundle-dev": "rollup -c ./rollup.dev.config.js",
    "bundle-dev-watch": "npm run bundle-dev -- -w",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "build-docs": "run-s compile api-extractor api-documenter vitepress-build",
    "build": "run-s build-docs bundle copy-res docs:patch",
    "standard-version": "standard-version",
    "standard-version-dryrun": "standard-version --dry-run",
    "git-push": "git push --follow-tags upstream trunk",
    "watch": "run-p compile-watch bundle-watch",
    "serve": "run-s docs:dev",
    "serve-index": "vite",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "npm run lint -- --fix",
    "test-karma": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test": "run-s build",
    "tag": "run-s standard-version git-push",
    "update": "npx npm-check-updates -u"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@codemirror/theme-one-dark": "^0.19.1",
    "@hpcc-js/util": "^2.40.0",
    "@lumino/dragdrop": "file:../lumino/packages/dragdrop",
    "@lumino/messaging": "file:../lumino/packages/messaging",
    "@lumino/widgets": "file:../lumino/packages/widgets",
    "@observablehq/inspector": "^3.2.4",
    "@observablehq/parser": "^6.0.0",
    "@observablehq/runtime": "^4.18.3",
    "d3": "^7.2.1",
    "d3-sankey": "^0.12.3",
    "preact": "^10.6.4",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@codemirror/basic-setup": "^0.19.1",
    "@codemirror/lang-html": "^0.19.4",
    "@codemirror/lang-json": "^0.19.1",
    "@microsoft/api-documenter": "^7.13.78",
    "@microsoft/api-extractor": "^7.19.3",
    "@microsoft/api-extractor-model": "^7.15.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/chai": "^4.3.0",
    "@types/d3": "^7.1.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-transform-tagged-template": "^0.0.3",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4",
    "vitepress": "^0.20.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/hpcc-js-wc.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/hpcc-js-wc/issues"
  },
  "homepage": "https://github.com/hpcc-systems/hpcc-js-wc#readme"
}